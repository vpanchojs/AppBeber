<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:bottomLeftRadius="20dip" android:bottomRightRadius="20dip" android:topLeftRadius="2dip" android:topRightRadius="2dip" />
            <stroke android:width="1dip" android:color="@color/dividerAccount" />
            <solid android:color="@color/dividerAccount" />
        </shape>
    </item>
