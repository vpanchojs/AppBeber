    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:bottomLeftRadius="20dip" android:bottomRightRadius="20dip" android:topLeftRadius="2dip" android:topRightRadius="2dip" />
            <stroke android:width="1dip" android:color="@color/dividerAccount" />
            <solid android:color="@color/coloBtnGreenDisable" />
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <corners android:bottomLeftRadius="20dip" android:bottomRightRadius="20dip" android:topLeftRadius="2dip" android:topRightRadius="2dip" />
            <stroke android:width="1dip" android:color="@color/dividerAccount" />
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
